# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4.1-mini
RESEND_API_KEY=

# Google OAuth desktop credentials or web credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost
GOOGLE_REFRESH_TOKEN=

# User details
GEE_USER_EMAIL=
GEE_USER_NAME=

# Delivery
GEE_TO_EMAIL=
# Optional override sender display name. Defaults to "Gee"
GEE_FROM_NAME=Gee
# Verified sender in Resend (for example hello@yourdomain.com)
GEE_FROM_EMAIL=
# Daily send time in UTC hour (0-23). Default 9 = 9:00 a.m. GMT
GEE_DAILY_SEND_HOUR_UTC=9

# Files
GEE_STATE_FILE=.gee-state.json
GEE_PREFERENCES_FILE=.gee-preferences.json

# Preferences web UI
GEE_WEB_PORT=8787

# Optional toggles
GEE_DRY_RUN=false
# Force welcome/onboarding email format even after first run
FORCE_WELCOME_EMAIL=false
